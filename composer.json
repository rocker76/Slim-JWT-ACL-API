	}],
	"require": {
		"php": ">=7.1",
		"monolog/monolog": "^1.17",
		"vlucas/phpdotenv": "^2.4",
		"tuupola/slim-jwt-auth": "^3.1",
		"slim/slim": "^3.11",
		"slim/php-view": "^2.2",
		"slim/http-cache": "^0.4.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "App"
		}
	},
	"config": {
		"preferred-install": "dist"
	},
	"repositories": {
		"packagist.org": {
			"type": "composer",
			"url": "https://packagist.org"
		}
	}
